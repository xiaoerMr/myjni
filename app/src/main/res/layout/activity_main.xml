<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/jni"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="Jni" />
    <Button
        android:id="@+id/socket"
        android:layout_toEndOf="@+id/jni"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="socket" />
    <Button
        android:id="@+id/net_state"
        android:layout_toEndOf="@+id/socket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="网络状态监听" />
    <Button
        android:id="@+id/read_file"
        android:layout_toEndOf="@+id/net_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="读取文件" />
    <Button
        android:id="@+id/dialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:layout_below="@+id/jni"
        android:text="弹出框" />

    <Button
        android:id="@+id/thread"
        android:layout_toEndOf="@+id/dialog"
        android:layout_below="@+id/jni"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="线程池" />

</RelativeLayout>